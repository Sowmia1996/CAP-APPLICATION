{
    "_version": "1.8.0",
    "sap.app": {
     "id": "browseBooks",
     "type": "application",
     "title": "Browse Books",
     "description": "Shop For Books",
     "dataSources": {
      "BooksCatalog": {
       "uri": "/api/browse/",
       "type": "OData",
       "settings": {
        "odataVersion": "4.0"
       }
      }
     }
    },
    "sap.ui5": {
     "dependencies": {
      "libs": {
       "sap.fe.templates": {}
      }
     },
     "models": {
      "": {
       "dataSource": "BooksCatalog",
       "settings": {
        "synchronizationMode": "None",
        "operationMode": "Server",
        "autoExpandSelect": true,
        "earlyRequests": true,
        "groupProperties": {
         "default": {
          "submit": "Auto"
         }
        }
       }
      }
     },
     "routing": {
      "routes": [
       {
        "pattern": ":?query:",
        "name": "BooksList",
        "target": "BooksList"
       }
      ],
      "targets": {
       "BooksList": {
        "type": "Component",
        "id": "BooksList",
        "name": "sap.fe.templates.ListReport",
        "options": {
         "settings": {
          "entitySet": "Books",
          "controlConfiguration": {
            "@com.sap.vocabularies.UI.v1.LineItem": {
                "columns": {
                    "AddReviewsColumn": {
                        "header": "Add Reviews",
                        "template": "browseBooks.custom.AddReviewButton"
                    }
                }
            }
          },
          "content": {
            "header": {
                "actions": {
                    "createOrder": {
                        "press": "browseBooks.custom.CustomHandler.createOrder",
                        "visible": true,
                        "enabled": true,
                        "text": "Add Items To Cart"                                       
                    }
                }
            }
          }
         }
        }
       }
      }
     }
    },
    "sap.ui": {
     "technology": "UI5",
     "fullWidth": false
    }
}